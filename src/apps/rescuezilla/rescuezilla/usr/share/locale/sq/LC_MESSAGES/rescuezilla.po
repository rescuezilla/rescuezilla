msgid "This may occur if Clonezilla was originally unable to backup this partition."
msgstr ""
"Kjo mund të ndodhë nëse Clonezilla s’qe fillimisht në gjendje të kopjeruante "
"këtë pjesë."

#, python-brace-format
msgid "Error restoring partition {image_key} to {destination_partition}."
msgstr "Gabim në rikthimin e pjesës {image_key} te {destination_partition}."

#, python-brace-format
msgid "Successfully restored image partition {image} to {destination_partition}"
msgstr "U rikthye me sukses pjesë pamjeje {image} te {destination_partition}"

