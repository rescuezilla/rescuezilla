msgstr "마운트 해제: {path}"

#, python-brace-format
msgid "Unable to unmount {path}"
msgstr "마운트 해제할 수 없음: {path}"

#, python-brace-format
msgid "Running: {app}"
msgstr "실행 중: {app}"

#, python-brace-format
